generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
  binaryTargets = ["linux-musl-openssl-3.0.x", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id  Int @id @default(autoincrement())
  public_id String
  name String
  username String
  summary String
}
