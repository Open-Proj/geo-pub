{
  "type": "module",
  "unstable": [ "temporal" ],
  "tasks": {
    "dev": "deno run --allow-net=0.0.0.0:8000 --allow-env --allow-read --allow-ffi --watch ./src/server.ts",
    "prisma:generate": "deno run -A 'npm:prisma' generate",
    "prisma:migrate": "deno run -A 'npm:prisma' migrate dev",
    "prisma:migrate:reset": "deno run -A 'npm:prisma' migrate reset"
  },
  "imports": {
    "@fedify/fedify": "jsr:@fedify/fedify@^1.1.0",
    "@fedify/postgres": "jsr:@fedify/postgres@^1.9.1",
    "@logtape/logtape": "jsr:@logtape/logtape@^1.2.0",
    "@prisma/client": "npm:@prisma/client@^6.19.0",
    "@std/assert": "jsr:@std/assert@1",
    "zod": "npm:zod@^4.1.12",
    "postgres": "npm:postgres@^3.4.7",
    "dotenv": "npm:dotenv@^17.2.3"
  }
}
